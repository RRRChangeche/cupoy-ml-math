   "id": "e47175b2-daaf-4bcd-b087-4ab246d72fea",
   "metadata": {},
   "outputs": [],
   "source": [
    "def L(θ):\n",
    "    return θ**2 * (1-θ)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "2459c46d-41b7-46fd-b6b3-e4e84fe5c708",
   "metadata": {},
   "outputs": [],
   "source": [
