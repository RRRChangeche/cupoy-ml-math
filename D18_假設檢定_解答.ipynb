   ],
   "source": [
    "# 計算臨界值\n",
    "mu = 100\n",
    "S = 10\n",
    "ctc_pt = 100 - 1.28 * (10 / np.sqrt(64))\n",
    "print('臨界值為%.2f'%ctc_pt)"
   ]
  },
  {
   "cell_type": "code",
